{
  "name": "cyberpunk",
  "displayName": "Cyberpunk Neon",
  "colors": {
    "primary": "#ff0080",
    "secondary": "#00ffff", 
    "accent": "#ffff00",
    "background": {
      "primary": "#0a0a0a",
      "secondary": "#1a0033",
      "tertiary": "#330066"
    },
    "text": {
      "primary": "#ffffff",
      "secondary": "#ff0080",
      "muted": "#cc00ff"
    },
    "border": "#ff0080",
    "success": "#00ff00",
    "warning": "#ffff00",
    "error": "#ff0040",
    "info": "#00ffff"
  },
  "gradients": {
    "primary": "linear-gradient(135deg, #ff0080, #00ffff)",
    "accent": "linear-gradient(135deg, #ffff00, #ff0080)",
    "hero": "linear-gradient(135deg, #ff0080 0%, #00ffff 50%, #ffff00 100%)",
    "card": "linear-gradient(145deg, #1a0033, #0a0a0a)"
  },
  "shadows": {
    "glow": "0 0 30px rgba(255, 0, 128, 0.6)",
    "card": "0 10px 30px rgba(255, 0, 128, 0.3)",
    "hover": "0 20px 40px rgba(0, 255, 255, 0.4)"
  },
  "typography": {
    "fontFamily": "'Orbitron', 'Courier New', monospace",
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem", 
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "1.875rem",
      "4xl": "2.25rem"
    }
  },
  "animations": {
    "duration": {
      "fast": "100ms",
      "normal": "200ms",
      "slow": "400ms"
    },
    "easing": {
      "ease": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
      "easeIn": "cubic-bezier(0.55, 0.085, 0.68, 0.53)",
      "easeOut": "cubic-bezier(0.25, 0.46, 0.45, 0.94)",
      "easeInOut": "cubic-bezier(0.445, 0.05, 0.55, 0.95)"
    }
  },
  "spacing": {
    "xs": "0.25rem",
    "sm": "0.75rem",
    "md": "1.25rem", 
    "lg": "2rem",
    "xl": "3.5rem",
    "2xl": "5rem"
  },
  "borderRadius": {
    "sm": "0.125rem",
    "md": "0.25rem",
    "lg": "0.5rem",
    "xl": "0.75rem",
    "2xl": "1rem",
    "full": "9999px"
  },
  "particles": [
    {
      "class": "w-1 h-1 bg-pink-500 animate-ping",
      "style": "top: 20%; left: 10%; animation-delay: 0s; filter: blur(0.5px);"
    },
    {
      "class": "w-2 h-2 bg-cyan-400 animate-pulse", 
      "style": "top: 60%; right: 15%; animation-delay: 0.5s; filter: blur(1px);"
    },
    {
      "class": "w-1.5 h-1.5 bg-yellow-400 animate-ping",
      "style": "bottom: 30%; left: 80%; animation-delay: 1s; filter: blur(0.8px);"
    },
    {
      "class": "w-0.5 h-0.5 bg-purple-500 animate-pulse",
      "style": "top: 45%; left: 60%; animation-delay: 1.5s;"
    },
    {
      "class": "w-1 h-1 bg-green-400 animate-ping",
      "style": "bottom: 60%; right: 40%; animation-delay: 2s; filter: blur(0.3px);"
    }
  ]
}